{
  "name": "helloS3",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "deploy-online": "serverless deploy -v",
    "deploy-debug": "SLS_DEBUG=t STAGE=test serverless deploy",
    "deploy-offline": "serverless offline --useSeparateProcesses",
    "invoke-local": "serverless invoke local -f hello",
    "lint": "node_modules/.bin/eslint --ext .ts ./ --fix --cache",
    "test": "node_modules/.bin/jest --coverage -u"
  },
  "dependencies": {
    "source-map-support": "^0.5.10"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.4",
    "jest": "^24.7.1",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
